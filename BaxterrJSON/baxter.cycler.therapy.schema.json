{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://www.davita.com/baxter.cycler.therapy.schema.json",
    "type": "object",
    "title": "Baxter Cycler Treatment",
    "description": "This is the json schema for a single actual therapy cycler treatment by a patient from the Baxter cycler.",
    "default": [],
    "examples": [
        [
            {
                "Therapy Cycle Time": "20:49:05",
                "Cycle Type": "Initial Drain",
                "CycleAttributes": {
                    "Fill Volume (mL)": 0,
                    "Fill Time (HH:MM)": "",
                    "Dwell Time (HH:MM)": "",
                    "Drain Time (HH:MM)": "00:02",
                    "Drain Volume (mL)": 34,
                    "UF/Cycle (mL)": 11
                }
            },
            {
                "Therapy Cycle Time": "20:51:09",
                "Cycle Type": "Night Cycle 1",
                "CycleAttributes": {
                    "Fill Volume (mL)": 2500,
                    "Fill Time (HH:MM)": "00:14",
                    "Dwell Time (HH:MM)": "01:21",
                    "Drain Time (HH:MM)": "00:20",
                    "Drain Volume (mL)": 2304,
                    "UF/Cycle (mL)": -196
                }
            }
        ]
    ],
    "properties": {
    	"Therapy Cycle Time": {
    		"type": "string",
            "maxLength": 10,
            "title": "The Therapy Cycle Time schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "20:49:05"
            ]
    	},
	    "Cycle Type": {
	      "type": "string",
          "title": "The Cycle Type schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
              "Initial Drain"
          ]
	    },
	    "CycleAttributes": {
	      "type": "object",
          "title": "The CycleAttributes schema",
          "description": "An explanation about the purpose of this instance.",
          "default": {},
          "examples": [
              {
                  "Fill Volume (mL)": 0,
                  "Fill Time (HH:MM)": "",
                  "Dwell Time (HH:MM)": "",
                  "Drain Time (HH:MM)": "00:02",
                  "Drain Volume (mL)": 34,
                  "UF/Cycle (mL)": 11
              }
          ],
	      "properties": {
		        "Fill Volume (mL)": {
		          "type": "integer",
                  "title": "The Fill Volume (mL) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      ""
                  ]
		        },
		        "Fill Time (HH:MM)": {
		          "type": "string",
                  "maxLength": 5,
                  "patern": "^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$",
                  "title": "The Fill Time (HH:MM) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      ""
                  ]
		        },
		        "Dwell Time (HH:MM)": {
		          "type": "string",
                  "maxLength": 5,
                  "patern": "^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$",
                  "title": "The Dwell Time (HH:MM) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      ""
                  ]
		        },
		        "Drain Time (HH:MM)": {
		          "type": "string",
                  "maxLength": 5,
                  "patern": "^([0-9]|0[0-9]|1[0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]$",
                  "title": "The Drain Time (HH:MM) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      "00:02"
                  ]
		        },
		        "Drain Volume (mL)": {
		          "type": "integer",
                  "title": "The Drain Volume (mL) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      "34"
                  ]
		        },
		        "UF/Cycle (mL)": {
		          "type": "integer",
                  "title": "The UF/Cycle (mL) schema",
                  "description": "An explanation about the purpose of this instance.",
                  "default": "",
                  "examples": [
                      "11"
                  ]
		        }
		      },
	      "required": [
	        "Fill Volume (mL)",
	        "Fill Time (HH:MM)",
	        "Dwell Time (HH:MM)",
	        "Drain Time (HH:MM)",
	        "Drain Volume (mL)",
	        "UF/Cycle (mL)"
	      ]
	    }
    },
	  "required": [
	    "Therapy Cycle Time",
	    "Cycle Type",
	    "CycleAttributes"
	  ]
}